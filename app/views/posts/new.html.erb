 <h1>New Post</h1>
  
<%= form_with model: post, url: user_posts_path, method: :post do |form| %>
  <div>
    <%= form.label :title %>
    <%= form.text_field :title %>
  </div>

  <div>
    <%= form.label :text %>
    <%= form.text_area :text %>
  </div>
  
  <button>
    <%= form.submit 'Create Post' %>
  </button>
<% end %>